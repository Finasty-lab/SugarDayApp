<UserControl x:Class="SugarDay.UC.ProfilButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SugarDay.UC"
             mc:Ignorable="d" 
             d:DesignHeight="23" d:DesignWidth="90">
    <Grid>
        <Button Style="{StaticResource zoneButton}" FontSize="13" Click="Profil_button" x:Name="LoginButton" x:FieldModifier="public">
            <TextBlock Text="Mon Profil" />
        </Button>
    </Grid>
</UserControl>
